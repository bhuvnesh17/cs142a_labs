// test if-else and while checks for boolean
func compare_if(a : int) : int {
  if (a) {
    ::printInt(7);
  }
  ::printInt(5);
  return 0;
}

func compare_while(a:int) : int {
  while (a) {
    ::printInt(7);
    let a = a - 1;
  }
  return 0;
}

func main() : void {
  ::compare_if(4);
  ::compare_while(8);
}
